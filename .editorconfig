# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
tab_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.coffee]
indent_size = 2

[*.js]
indent_size = 2

[*.json]
indent_size = 2

